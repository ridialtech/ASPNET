@model IEnumerable<AppDiti2025.Models.Admin>
@{
    ViewData["Title"] = "Admins";
}
<h1>Admins</h1>
<table class="table">
    <thead>
        <tr>
            <th>Nom</th>
            <th>Prenom</th>
            <th>Email</th>
            <th>Role</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model)
{
        <tr>
            <td>@item.Nom</td>
            <td>@item.Prenom</td>
            <td>@item.Email</td>
            <td>@item.Role</td>
        </tr>
}
    </tbody>
</table>
